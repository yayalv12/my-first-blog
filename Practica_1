#my first function
def hi(): 
    print('Hi there!')
    print('How are you?')

#my second fuction with IF
def comp(volume):
    if volume < 20:
        print("It's kinda quiet.")
    elif 20 <= volume < 40:
        print("It's nice for background music")
    elif 40 <= volume < 60:
        print("Perfect, I can hear all the details")
    elif 60 <= volume < 80:
        print("Nice for parties")
    elif 80 <= volume < 100:
        print("A bit loud!")
    else:
        print("My ears are hurting! :(")

#Third fuction whit FOR (loops)
def ciclo(name):
    print('Hi ' + name + '!')

#main procedure
girls = ['Rachel', 'Monica', 'Phoebe', 'Ola', 'You']
print("Hello,Django girls. I am super exited to be here :)")

hi() #llamando a funcion hi
comp(57) 

for name in girls:
    ciclo(name)
    print('Next girl')

for i in range(10,20):
    print(i)